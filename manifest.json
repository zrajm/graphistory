{
  "description": "EXPERIMENTAL: Save/restore tabs including page history.",
  "manifest_version": 2,
  "name": "Graphistory",
  "version": "0.1",
  "homepage_url": "https://zrajm.github.io/graphistory",
  "icons": {
    "8": "icon-8.png",
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png",
    "192": "icon-192.png",
    "512": "icon-512.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "theme_icons": [{
        "light": "icon-32.png",
        "dark": "icon-32.png",
        "size": 32
    }],
    "default_title": "Graphistory",
    "default_popup": "popup.html"
  },
  "permissions": ["<all_urls>", "activeTab", "storage", "webNavigation"],
  "background": {
    "scripts": ["background.js"]
  }
}
